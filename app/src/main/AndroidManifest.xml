<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:label="@string/MatchPoint"
        android:theme="@style/Theme.MyApplication">
        <activity android:name=".clubhouse.VerifyActivity" />
        <activity android:name=".clubhouse.AdditionalInfo" />
        <activity android:name=".clubhouse.LoginActivity" />
        <activity android:name=".clubhouse.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>